<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <style type="text/css">
      html { height: 100% }
      body { height: 100%; margin: 0; padding: 0 }
      #map-canvas { height: 100% }
    </style>
  </head>
  <body>

 <form>
    <input type="text" class="yolo">
    <input type="submit" />
  </form>
<p>hi</p>
<div id="map-canvas"/>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GMAPS_API_KEY'] %>&sensor=false">
    </script>
    <script type="text/javascript">

      function initialize() {
        var myLatlng = new google.maps.LatLng(-34.397, 150.644);
        var otherLatlng = new google.maps.LatLng(-34.397, 151.644);
        var mapOptions = {
          center: myLatlng,
          zoom: 8
        };
        var map = new google.maps.Map(document.getElementById("map-canvas"),
            mapOptions);

        var marker = new google.maps.Marker({
            position: otherLatlng,
            map: map,
            title:"Blob!"
        });

        var newMarker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title:"Hello World!"
        });
      }
      google.maps.event.addDomListener(window, 'load', initialize);

    </script>

    <script type="text/javascript">
      $('form').on('submit', function (e) {
        e.preventDefault();
        $.post( "/practice2", { name: $(".yolo").val(), time: "2pm" }, function (data) {
          console.log('hello', data);
        });
      });
    </script>

    </body>
</html>
